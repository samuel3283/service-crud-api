{
  "family": "crud-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::329631044416:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::329631044416:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "app",
      "image": "329631044416.dkr.ecr.us-east-1.amazonaws.com/service-crud-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ]
}
