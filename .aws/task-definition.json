{
  "family": "crud-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::278396816620:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::278396816620:role/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "crud-app",
      "image": "278396816620.dkr.ecr.us-east-1.amazonaws.com/crud-api:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/crud-app",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}